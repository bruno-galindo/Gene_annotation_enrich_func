        <rdf:value>&lt;div data-citation-items=&quot;%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F11568088%2Fitems%2F79822ZMI%22%5D%2C%22itemData%22%3A%7B%22id%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F11568088%2Fitems%2F79822ZMI%22%2C%22type%22%3A%22book%22%2C%22call-number%22%3A%22QP246%20.A39%202002%22%2C%22edition%22%3A%221st%20ed%22%2C%22event-place%22%3A%22Ames%2C%20Iowa%22%2C%22ISBN%22%3A%22978-0-8138-2992-0%22%2C%22language%22%3A%22en%22%2C%22number-of-pages%22%3A%22278%22%2C%22publisher%22%3A%22Iowa%20State%20University%20Press%22%2C%22publisher-place%22%3A%22Ames%2C%20Iowa%22%2C%22source%22%3A%22Library%20of%20Congress%20ISBN%22%2C%22title%22%3A%22Lactation%20and%20the%20mammary%20gland%22%2C%22author%22%3A%5B%7B%22family%22%3A%22Akers%22%2C%22given%22%3A%22R.%20Michael%22%7D%5D%2C%22issued%22%3A%7B%22date-parts%22%3A%5B%5B%222002%22%5D%5D%7D%7D%7D%5D&quot; data-schema-version=&quot;8&quot;&gt;&lt;p&gt;&lt;span class=&quot;highlight&quot; data-annotation=&quot;%7B%22attachmentURI%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F11568088%2Fitems%2FADB7NFML%22%2C%22annotationKey%22%3A%2236I46JJ9%22%2C%22color%22%3A%22%235fb236%22%2C%22pageLabel%22%3A%2225%22%2C%22position%22%3A%7B%22pageIndex%22%3A24%2C%22rects%22%3A%5B%5B339.546%2C627.579%2C431.874%2C639.413%5D%2C%5B77.005%2C613.895%2C431.962%2C625.729%5D%2C%5B77.17%2C600.459%2C137.548%2C612.289%5D%5D%7D%2C%22citationItem%22%3A%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F11568088%2Fitems%2F79822ZMI%22%5D%2C%22locator%22%3A%2225%22%7D%7D&quot;&gt;“However, the foundation for the dramatic mammogenesis during these stages begins when the animal is an early fetus.”&lt;/span&gt; &lt;span class=&quot;citation&quot; data-citation=&quot;%7B%22citationItems%22%3A%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F11568088%2Fitems%2F79822ZMI%22%5D%2C%22locator%22%3A%2225%22%7D%5D%2C%22properties%22%3A%7B%7D%7D&quot;&gt;(&lt;span class=&quot;citation-item&quot;&gt;Akers, 2002, p. 25&lt;/span&gt;)&lt;/span&gt;&lt;/p&gt;
&lt;p&gt;&lt;span class=&quot;highlight&quot; data-annotation=&quot;%7B%22attachmentURI%22%3A%22http%3A%2F%2Fzotero.org%2Fusers%2F11568088%2Fitems%2FADB7NFML%22%2C%22annotationKey%22%3A%223SFQXTXF%22%2C%22color%22%3A%22%23e56eee%22%2C%22pageLabel%22%3A%2225%22%2C%22position%22%3A%7B%22pageIndex%22%3A24%2C%22rects%22%3A%5B%5B140.198%2C600.459%2C432.404%2C612.289%5D%2C%5B77.138%2C587.259%2C394.629%2C599.092%5D%5D%7D%2C%22citationItem%22%3A%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F11568088%2Fitems%2F79822ZMI%22%5D%2C%22locator%22%3A%2225%22%7D%7D&quot;&gt;“Fundamentally, mammary development can be developmentally considered a joining of the epithelial ectoderm and the underlying mesoderm.”&lt;/span&gt; &lt;span class=&quot;citation&quot; data-citation=&quot;%7B%22citationItems%22%3A%5B%7B%22uris%22%3A%5B%22http%3A%2F%2Fzotero.org%2Fusers%2F11568088%2Fitems%2F79822ZMI%22%5D%2C%22locator%22%3A%2225%22%7D%5D%2C%22properties%22%3A%7B%7D%7D&quot;&gt;(&lt;span class=&quot;citation-item&quot;&gt;Akers, 2002, p. 25&lt;/span&gt;)&lt;/span&gt;&lt;/p&gt;
&lt;/div&gt;</rdf:value>
